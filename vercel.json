{
	"version": 2,
	"build": {
		"env": {
			"BUILDING_FOR_NOW": "true"
		}
	},
	"builds": [
		{ "src": "blog/package.json", "use": "@vercel/next" },
		{ "src": "home/package.json", "use": "@vercel/next" }
	],
	"routes": [
		{ "src": "/:path*", "dest": "/:path*" },
		{ "src": "/blog", "dest": "https://multi-zone-blog-rose.vercel.app/blog" },
		{
			"src": "/blog/:path*",
			"dest": "https://multi-zone-blog-rose.vercel.app/blog/:path*"
		}
	]
}
